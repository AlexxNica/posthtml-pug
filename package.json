{
  "name": "posthtml-pug",
  "description": "Pug for PostHTML",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "dependencies": {
    "posthtml-parser": "^0.2.0",
    "pug": "^2.0.0-beta6"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "nyc": "^8.3.0",
    "posthtml": "^0.9.0",
    "standard": "^8.2.0",
    "standard-changelog": "0.0.1"
  },
  "scripts": {
    "clean": "rm -rf .nyc_output coverage jsdoc-api",
    "lint": "standard",
    "pretest": "npm run clean && npm run lint",
    "test": "nyc ava 'test/index.js'",
    "logs": "standard-changelog > CHANGELOG.md",
    "docs": "jsdoc2md index.js > INDEX.md",
    "release:major": "mversion major",
    "release:minor": "mversion minor",
    "release:patch": "mversion patch"
  },
  "ava": {
    "verbose": "true"
  },
  "nyc": {
    "all": true,
    "include": [
      "index.js",
      "test"
    ],
    "extension": [
      ".js"
    ]
  },
  "keywords": [
    "pug",
    "html",
    "posthtml",
    "posthtmlparser"
  ],
  "author": {
    "name": "Michael Ciniawky",
    "email": "michael.ciniawsky@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/posthtml/posthtml-pug"
  },
  "bugs": {
    "url": "https://github.com/posthtml/posthtml-pug/issues"
  },
  "homepage": "https://github.com/posthtml/posthtml-pug#readme",
  "license": "MIT"
}
